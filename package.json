{
  "name": "@portkey-ai/gateway",
  "version": "1.9.17",
  "description": "A fast AI gateway by Portkey",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Portkey-AI/gateway.git"
  },
  "keywords": [
    "AI gateway",
    "Portkey",
    "Portkey AI",
    "LLM",
    "OpenAI"
  ],
  "author": "portkey-ai <support@portkey.ai>",
  "license": "MIT",
  "files": [
    "build",
    "README.md",
    "SECURITY.md",
    "LICENSE",
    "build/public"
  ],
  "scripts": {
    "dev": "wrangler dev src/index.ts",
    "dev:node": "tsx src/start-server.ts",
    "dev:workerd": "wrangler dev src/index.ts",
    "deploy": "wrangler deploy --minify src/index.ts",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "build": "rollup -c",
    "build-bun": "bun build --target=bun --outfile=build/llm_gateway.js src/start-server.ts",
    "build-plugins": "tsx plugins/build.ts",
    "prepublishOnly": "npm run build",
    "start:node": "node build/start-server.js",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "prettier --check \"./**/*.{js,jsx,ts,tsx,json}\"",
    "test:gateway": "jest src/",
    "test:plugins": "jest plugins/",
    "pre-push": "npm run build && node start-test.js",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production'){process.exit(1)} \" || husky",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@cfworker/json-schema": "^4.1.1",
    "@hono/node-server": "^1.13.8",
    "@hono/node-ws": "^1.1.0",
    "@portkey-ai/mustache": "^2.1.2",
    "@smithy/signature-v4": "^2.3.0",
    "@types/mustache": "^4.2.5",
    "async-retry": "^1.3.3",
    "avsc": "^5.7.7",
    "hono": "^4.6.10",
    "ws": "^8.18.0",
    "zod": "^3.22.4",
    "patch-package": "^8.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250224.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/async-retry": "^1.4.9",
    "@types/jest": "^29.5.14",
    "@types/node": "20.8.3",
    "@types/ws": "^8.5.14",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "rollup": "^4.34.8",
    "rollup-plugin-copy": "^3.5.0",
    "ts-jest": "^29.2.6",
    "tsx": "^4.19.3",
    "typescript-eslint": "^8.25.0",
    "wrangler": "^3.110.0"
  },
  "bin": "build/start-server.js",
  "type": "module"
}
